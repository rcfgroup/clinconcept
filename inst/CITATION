citHeader("To cite clinconcept in publications use:")

citEntry(entry = "Article",
  title        = "Simpler handling of clinical concept dictionaries in R with {clinconcept}",
  author       = personList(as.person("Robert C. Free"),
                   as.person("X")),
  journal      = "Journal of Statistical Software",
  year         = "2020",
  volume       = "40",
  number       = "3",
  pages        = "1--25",
  url          = "http://www.jstatsoft.org/v40/i03/",

  textVersion  =
  paste("Robert C. Free, X (2020).",
        "Simpler handling of clinical concept dictionaries in R with {clinconcept}",
        "Journal of Statistical Software, 40(3), 1-25.",
        "URL http://www.jstatsoft.org/v40/i03/.")
)
